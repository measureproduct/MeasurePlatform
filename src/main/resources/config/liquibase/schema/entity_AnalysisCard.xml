<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

	<property name="now" value="now()" dbms="mysql,h2" />
	<property name="now" value="current_timestamp" dbms="postgresql" />
	<property name="now" value="sysdate" dbms="oracle" />

	<property name="autoIncrement" value="true"
		dbms="mysql,h2,postgresql,oracle" />

	<property name="floatType" value="float4" dbms="postgresql, h2" />
	<property name="floatType" value="float" dbms="mysql, oracle" />

	<!-- Added the entity AlertEvent. -->
	<changeSet id="20180131133433-1" author="softeam">
		<createTable tableName="analysiscard">
			<column name="id" type="bigint" autoIncrement="${autoIncrement}">
				<constraints primaryKey="true" nullable="false" />
			</column>
			
			<column name="prefered_width" type="int">
				<constraints primaryKey="true" nullable="false" />
			</column>
			
			<column name="prefered_height" type="int">
				<constraints primaryKey="true" nullable="false" />
			</column>

			<column name="card_label" type="varchar(255)">
				<constraints nullable="false" />
			</column>


			<column name="card_url" type="varchar(255)">
				<constraints nullable="false" />
			</column>

			<column name="projectanalysis_id" type="bigint">
				<constraints nullable="true" />
			</column>


		</createTable>
	</changeSet>
</databaseChangeLog>
